{
   "template": "browbeat-logstash-*",
   "mappings": {
       "result": {
           "properties": {
               "action": {
                   "index": "true",
                   "type": "keyword"
                },
               "browbeat_cloud_name": {
                   "index": "true",
                   "type": "keyword"
                },
               "message": {
                   "properties": {
                       "openstack-general-system-performance": {
                           "index": "true",
                           "type": "keyword"
                        }
                    }
                },
               "level": {
                   "type": "text",
                   "index": true
                },
               "hostname": {
                   "index": "true",
                   "type": "keyword"
                },
               "timestamp": {
                   "type": "date",
                   "format": "strict_date_optional_time||epoch_millis"
                }
            }
        }
    }
}
