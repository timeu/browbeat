{
    "template": "browbeat-rally-*",
    "settings": {
        "index.mapping.total_fields.limit": 5000,
        "number_of_shards": 1,
        "number_of_replicas": 0
    },
    "mappings": {
        "_doc": {
            "properties": {
                "type": { "type": "keyword" },
                "action": {
                    "index": "true",
                    "type": "keyword"
                },
                "action_name": {
                    "index": "true",
                    "type": "keyword"
                },
                "browbeat_uuid": {
                    "index": "true",
                    "type": "keyword"
                },
                "cloud_name": {
                    "index": "true",
                    "type": "keyword"
                },
                "grafana_url": {
                    "properties": {
                        "openstack-general-system-performance": {
                            "index": "true",
                            "type": "keyword"
                        }
                    }
                },
                "result": {
                    "index": true,
                    "type": "text"
                },
                "error_msg": {
                    "index": "true",
                    "type": "keyword"
                },
                "error_type": {
                    "index": "true",
                    "type": "keyword"
                },"scenario": {
                    "index": "true",
                    "type": "keyword"
                },
                "timestamp": {
                    "type": "date",
                    "format": "strict_date_optional_time||epoch_millis"
                }
            }
        }   
    }
}
